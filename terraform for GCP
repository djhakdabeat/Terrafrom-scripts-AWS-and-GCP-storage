provider "google" {
  project = "<YOUR_PROJECT_ID>"
  region  = "us-east1"
}

resource "google_storage_bucket" "secure_bucket" {
  name          = "my-secure-data-lake"
  location      = "US"
  force_destroy = true

  uniform_bucket_level_access = true

  encryption {
    default_kms_key_name = "<YOUR_KMS_KEY>"
  }
}
